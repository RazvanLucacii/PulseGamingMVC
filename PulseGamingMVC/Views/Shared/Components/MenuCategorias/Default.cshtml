@model List<Genero>

<li class="nav-item dropdown botonesCustom">
    <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown">
        Categorias
    </a>
    <ul class="dropdown-menu">
        @foreach (Genero genero in Model)
        {
            <li>
                <a class="dropdown-item" asp-action="ListarJuegosCategorias" asp-controller="Juegos" asp-route-idgenero="@genero.IdGenero">
                    @genero.NombreGenero
                </a>
            </li>
        }
    </ul>
</li>
